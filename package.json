{
  "name": "rpps-api",
  "version": "1.0.0",
  "description": "API donnant accès au répertoire RPPS (Répertoire Partagé des Professionnels intervenant dans le système de santé)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src -e js,ts,json --exec \"ts-node src/index.ts\"",
    "build": "tsup src/index.ts --format cjs --clean",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SwanGon/rpps-api.git"
  },
  "author": "Swan Gonzales",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SwanGon/rpps-api/issues"
  },
  "homepage": "https://github.com/SwanGon/rpps-api#readme",
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
